docker compose exec core npm run migration:generate -- src/database/migrations/
docker compose exec db psql -U hoang -d social_commerce_management -c "select id from conversations  where status = 'closed';"
docker compose exec db psql -U hoang -d social_commerce_management -c "select * from visitors;"
docker compose exec db psql -U hoang -d social_commerce_management -c "select * from conversations where visitor_id = 15;"



docker compose exec db psql -U hoang -d social_commerce_management -c "select * from messages  where conversation_id in (select id from conversations where status = 'closed');"

docker compose exec core npm run migration:run
